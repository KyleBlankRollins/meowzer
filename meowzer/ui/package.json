{
  "name": "@meowzer/ui",
  "version": "0.1.0",
  "description": "Lit Element + Quiet UI components for Meowzer SDK - drop-in cat animations for any website",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./providers": {
      "import": "./dist/providers/index.js",
      "types": "./dist/providers/index.d.ts"
    },
    "./components/*": {
      "import": "./dist/components/*/index.js",
      "types": "./dist/components/*/index.d.ts"
    }
  },
  "scripts": {
    "dev": "tsc --watch",
    "build": "tsc",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "web-components",
    "lit-element",
    "cats",
    "animation",
    "ui-library",
    "meowzer"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "lit": "^3.0.0",
    "@lit/context": "^1.1.0",
    "@quietui/quiet": "^1.3.0",
    "meowzer": "file:../sdk"
  },
  "devDependencies": {
    "@types/node": "^20.11.19",
    "@vitest/ui": "^4.0.2",
    "happy-dom": "^20.0.8",
    "typescript": "^5.3.3",
    "vitest": "^4.0.2"
  }
}
